# An example controller showing a sample home page
GET     /                           controllers.SignUpController.index()

+ nocsrf
POST 	/user/signup							controllers.SignUpController.signUpUser

GET		/user/:id							controllers.SignUpController.findSpecificUser(id: Int)
+ nocsrf
POST	/user/signin							controllers.SignUpController.signinUser


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
